{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "os": "#ACB0BE",
    "closer": "p:os",
    "pink": "#F5C2E7",
    "lavender": "#B4BEFE",
    "blue": "#89B4FA",
    "text": "#22232e"
  },
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "p:blue",
          "foreground": "p:text",
          "powerline_symbol": "\ue0b4",
          "leading_diamond": "\ue0b6",
          "style": "diamond",
          "template": "<#030304>{{.Icon}}</> {{ .UserName }} <#030304>\ufcbe</> {{ .HostName }} ",
          "type": "os"
        },
        {
          "background": "p:pink",
          "foreground": "p:text",
          "powerline_symbol": "\ue0b4",
          "leading_diamond": "\ue0b6",
          "style": "powerline",
          "properties": {
            "style": "dallas",
            "threshold": 0
          },
          "template": " <#030304>\uf252</> {{ .FormattedMs }}s ",
          "type": "executiontime"
        },
        {
          "properties": {
            "root_icon": "\uf292"
          },
          "style": "powerline",
          "template": "\uf0e7",
          "type": "root"
        },
        {
          "background": "p:pink",
          "foreground": "p:text",
          "powerline_symbol": "\ue0b4",
          "style": "powerline",
          "template": "<#030304>\uf85a</> RAM: {{ round .PhysicalPercentUsed .Precision }}% | {{ (div ((sub .PhysicalTotalMemory .PhysicalFreeMemory)|float64) 1000000000.0) }}/{{ (div .PhysicalTotalMemory 1000000000.0) }}GB ",
          "type": "sysinfo"
        },
        {
          "background": "p:lavender",
          "foreground": "p:text",
          "powerline_symbol": "\ue0b4",
          "properties": {
            "branch_icon": "\ue725 ",
            "cherry_pick_icon": "\ue29b ",
            "commit_icon": "\uf417 ",
            "fetch_status": false,
            "fetch_upstream_icon": false,
            "merge_icon": "\ue727 ",
            "no_commits_icon": "\uf594 ",
            "rebase_icon": "\ue728 ",
            "revert_icon": "\uf0e2 ",
            "tag_icon": "\uf412 ",
            "fetch_stash_count": true
          },
          "style": "powerline",
          "template": " <#030304>{{ .UpstreamIcon }}</>{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} <#030304>\uf044</> {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} <#030304>\uf046</> {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} <#030304>\uf692</> {{ .StashCount }}{{ end }} ",
          "type": "git"
        },
        {
          "foreground": "p:pink",
          "style": "plain",
          "template": "[{{ .Profile }}{{if .Region}}@{{ .Region }}{{ end }}]",
          "type": "aws"
        },
        {
          "foreground": "p:pink",
          "style": "plain",
          "template": "[{{.Context}}{{if .Namespace}} :: {{.Namespace}}{{end}}]",
          "type": "kubectl"
        },
        {
          "foreground": "p:pink",
          "style": "plain",
          "template": "[\uf0e7]",
          "type": "root"
        },
        {
          "foreground": "p:pink",
          "style": "powerline",
          "template": "[<#030304>\ufc8d</> Error, check your command]",
          "type": "exit"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "newline": false,
      "segments": [
        {
          "background": "p:lavender",
          "foreground": "p:text",
          "leading_diamond": "\uE0B6",
          "trailing_diamond": "\ue0b4",
          "style": "diamond",
          "template": "{{ if eq \"False\" (title (default \"False\" .Env.DISABLE_SEGMENT_PROJECT_NODE)) }}\uE718 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}{{ end }}",
          "type": "node"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "p:pink",
          "properties": {
            "style": "full"
          },
          "style": "plain",
          "template": "\uf07b {{ .Path }}",
          "type": "path"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "p:pink",
          "style": "plain",
          "template": "\u2514\u2500Î”",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 2
}