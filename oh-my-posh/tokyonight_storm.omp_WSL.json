{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "version": 3,
  "final_space": true,
  "palette": {
    "baseGrey": "#96979b",
    "blue": "#89B4FA",
    "closer": "p:os",
    "lavender": "#B4BEFE",
    "os": "#ACB0BE",
    "pink": "#F5C2E7",
    "text": "#22232e",
    "toxic": "#39fe54"
  },
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "os",
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "foreground": "p:text",
          "background": "p:blue",
          "properties": {
            "cache_duration": "1m"
          },
          "template": " <#030304>{{ .Icon }}</> {{ .UserName }} "
        },
        {
          "type": "path",
          "style": "powerline",
          "powerline_symbol": "\ue0b4",
          "foreground": "p:text",
          "background": "p:pink",
          "properties": {
            "style": "full",
            "cache_duration": "1s"
          },
          "template": " \uf07b {{ .Parent }}\u001b[1m<b>{{ .Folder }}</b>\u001b[22m"
        },
        {
          "type": "git",
          "style": "powerline",
          "powerline_symbol": "\ue0b4",
          "foreground": "p:text",
          "background": "p:lavender",
          "properties": {
            "cache_duration": "5s",
            "branch_icon": "\ue725 ",
            "cherry_pick_icon": "\ue29b ",
            "commit_icon": "\uf417 ",
            "fetch_stash_count": false,
            "fetch_status": false,
            "fetch_upstream_icon": false,
            "merge_icon": "\ue727 ",
            "no_commits_icon": "\uf594 ",
            "rebase_icon": "\ue728 ",
            "revert_icon": "\uf0e2 ",
            "tag_icon": "\uf412 "
          },
          "template": " <#030304>{{ .UpstreamIcon }}</>{{ .HEAD }}{{ if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} <#030304>\uf044</> {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} <#030304>\uf046</> {{ .Staging.String }}{{ end }} "
        },
        {
          "type": "aws",
          "style": "plain",
          "foreground": "p:pink",
          "properties": {
            "cache_duration": "30s"
          },
          "template": "{{ if .Env.AWS_PROFILE }}[{{ .Profile }}{{ if .Region }}@{{ .Region }}{{ end }}]{{ end }}"
        },
        {
          "type": "kubectl",
          "style": "plain",
          "foreground": "p:pink",
          "properties": {
            "cache_duration": "30s"
          },
          "template": "{{ if or .Env.KUBECONFIG .Env.KUBE_CONTEXT }}[{{ .Context }}{{ if .Namespace }} :: {{ .Namespace }}{{ end }}]{{ end }}"
        },
        {
          "type": "root",
          "style": "plain",
          "foreground": "p:pink",
          "properties": {
            "cache_duration": "1m"
          },
          "template": "{{ if .Root }}[\uf0e7]{{ end }}"
        },
        {
          "type": "exit",
          "style": "plain",
          "foreground": "p:pink",
          "properties": {
            "cache_duration": "none"
          },
          "template": "{{ if gt .Code 0 }}[Error, check your command]{{ end }}"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "type": "python",
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "foreground": "p:lavender",
          "background": "p:text",
          "properties": {
            "cache_duration": "1m",
            "display_mode": "environment",
            "display_virtual_env": true,
            "fetch_virtual_env": true
          },
          "template": "<#39fe54>\ue606</> {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }}"
        },
        {
          "type": "node",
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "foreground": "p:lavender",
          "background": "p:text",
          "properties": {
            "cache_duration": "1m"
          },
          "template": "{{ if eq \"False\" (title (default \"False\" .Env.DISABLE_SEGMENT_PROJECT_NODE)) }}<#39fe54>\udb80\udf99</> {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}{{ end }}"
        },
        {
          "type": "executiontime",
          "style": "plain",
          "foreground": "p:lavender",
          "properties": {
            "always_enabled": true,
            "cache_duration": "none"
          },
          "template": "<#F5C2E7>\uf252</> {{ .FormattedMs }} "
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "foreground": "p:pink",
          "properties": {
            "cache_duration": "none"
          },
          "template": "$"
        }
      ]
    }
  ]
}